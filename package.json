{
  "name": "@chubbyts/chubbyts-http-multipart",
  "type": "module",
  "version": "1.0.1",
  "description": "Chubbyts Http multipart request handling",
  "keywords": [
    "chubbyts",
    "http",
    "multipart",
    "middleware"
  ],
  "author": "Dominik Zogg",
  "license": "MIT",
  "repository": "chubbyts/chubbyts-http-multipart",
  "scripts": {
    "build": "node ./build.js",
    "cs-fix": "prettier --write src tests",
    "cs": "prettier --check src tests",
    "infection": "stryker run",
    "lint-fix": "eslint src tests --fix",
    "lint": "eslint src tests",
    "prepare": "npm run build",
    "test": "jest"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 100
      }
    },
    "coveragePathIgnorePatterns": [
      "src/index.ts"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./*": {
      "types": "./*.d.ts",
      "require": "./*.cjs",
      "import": "./*.mjs",
      "default": "./*.mjs"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@chubbyts/chubbyts-http-types": "^1.2.3",
    "busboy": "^1.6.0"
  },
  "devDependencies": {
    "@chubbyts/chubbyts-eslint": "^2.0.3",
    "@chubbyts/chubbyts-function-mock": "^1.4.1",
    "@chubbyts/chubbyts-packaging": "^2.0.5",
    "@stryker-mutator/core": "^8.0.0",
    "@stryker-mutator/jest-runner": "^8.0.0",
    "@types/busboy": "^1.5.3",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.2",
    "@types/qs": "^6.9.11",
    "form-data": "^4.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "qs": "^6.11.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
